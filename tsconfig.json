{
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ],

    "module": "ESNext",
    "target": "ES6",

    "allowImportingTsExtensions": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true
  }
}
